<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for post in site.posts %}
  <url>
    <loc>http://marklodato.github.com{{ post.id }}.html</loc>
    <lastmod>{{ post.date | date: "%Y-%m-%d" }}</lastmod>
  </url>
  {% endfor %}
  <url>
    <loc>http://marklodato.github.com</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
  </url>
  <url>
    <loc>http://marklodato.github.com/about.html</loc>
    <lastmod>2009-08-30</lastmod>
  </url>
  <url>
    <loc>http://marklodato.github.com/archive.html</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
  </url>
</urlset> 
